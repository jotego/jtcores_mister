            <part name="tr_13.7l" crc="a79be1eb" map="12"/>
        </interleave>
        <!-- SCR_START -->
        <!-- tiles - starts at 0x48000 - length 0x100000 (20 bits) -->
        <interleave output="16">
            <part name="tr-03a.2f" crc="1e0537ea" map="01"/>
            <part name="tr-01a.3f" crc="a8aa2e59" map="10"/>
        </interleave>
        <interleave output="16">
